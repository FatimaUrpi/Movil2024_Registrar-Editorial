<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:weightSum="12"
    android:padding="10dp">



    <TextView
        android:id="@+id/itemIdEditorial"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center|center"
        android:textSize="12sp"
        android:layout_weight="2"
        android:textStyle="bold"
        />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_weight="10"
        android:weightSum="4">


        <TextView
            android:id="@+id/itemRazonSocialEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/itemDireccionEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/itemRucEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/itemFechaCreacionEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/itemFechaRegistroEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/itemEstadoEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />


        <TextView
            android:id="@+id/itemPaisEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/itemCategoriaEditorial"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textSize="12sp"
            android:layout_weight="1"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.appcompat.widget.LinearLayoutCompat>